<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/86301d75-fbb5-42c6-8e1b-52c20245dd63/resourceGroups/RepairShop/providers/Microsoft.Web/sites/repairShop</ResourceId>
    <ResourceGroup>RepairShop</ResourceGroup>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://repairshop.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>repairshop.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>repairShop</DeployIisAppPath>
    <RemoteSitePhysicalPath />
