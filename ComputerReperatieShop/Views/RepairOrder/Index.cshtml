@model IEnumerable<ComputerRepairShop.Data.Models.RepairOrder>

@{
    ViewBag.Title = "Index";
}

<h2>Repair Order</h2>
<table class="table">
    <thead>
        <th> Name </th>
        <th>status</th>
        <th>start date</th>
        <th>end date</th>
    </thead>

    @foreach (var repairOrder in Model)
    {
       <tr> 
           <td>@repairOrder.Name</td>
           <td>@repairOrder.Status</td>
           <td>@repairOrder.StartDate</td>
           <td>@repairOrder.EndDate</td>
       </tr>
    }
</table>

